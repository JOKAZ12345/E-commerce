		var getCookie = function (sKey) {
				return decodeURIComponent(document.cookie.replace(new RegExp("(?:(?:^|.*;)\\s*" + encodeURIComponent(sKey).replace(/[\-\.\+\*]/g, "\\$&") + "\\s*\\=\\s*([^;]*).*$)|^.*$"), "$1")) || null;
			},
			
			SSO_token = getCookie('skyCFsidlf01'),

			networkConfig = {
				network: 'bskyb.fyre.co',
				authDelegate: new fyre.conv.RemoteAuthDelegate()
			},
			convConfig = {
				siteId: '360818',
				articleId: '1068514',
				el: 'livecomments-1068514',
				collectionMeta: 'eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJ1cmwiOiJodHRwOi8vbmV3cy5za3kuY29tL3N0b3J5LzEwNjg1MTQiLCJ0YWdzIjoiIiwidGl0bGUiOiJTcGFpbjogRXZpY3Rpb24gT3JkZXJzIFRvcCA3NSwwMDAgSW4gMjAxMiIsImFydGljbGVJZCI6MTA2ODUxNCwidHlwZSI6ImxpdmVjb21tZW50cyJ9.1LcV0-uRvU9NpP9dfGDDtPcnEsME2l5f8cu8qeEZiGk'
			};

		networkConfig.authDelegate.login = function (delegate) {
			if (SSO_token) {
				fyre.conv.login(SSO_token);
				delegate.success();
			} else {
				window.location.assign('https://skyid.sky.com/signin/news?successUrl=' + window.location.href);
			}
		};

		networkConfig.authDelegate.logout = function (delegate) {
			window.location.assign('https://skyid.sky.com/signout/news?successUrl=' + window.location.href);
		};

		fyre.conv.load(
			networkConfig,
			[convConfig],
			function () {
				if (SSO_token) {
					fyre.conv.login(SSO_token);
				}
			}
		);
	</script>


			

			<!-- end "wide primary" wrapper -->
		</div>
		<!-- end "normal" wrapper -->

		

